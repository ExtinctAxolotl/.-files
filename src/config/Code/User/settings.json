{
  "editor.cursorStyle": "line",
    "editor.lineNumbers": "relative",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "window.menuBarVisibility": "toggle",
    "editor.fontFamily": "VictorMono Nerd Font",
    "editor.fontLigatures": true,
    "terminal.external.linuxExec": "gnome-terminal",
    "window.title": "(  ͡° ͜ʖ ͡°)_/¯${activeEditorShort} ${dirty} ${separator}${remoteName}",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "editor.cursorBlinking": "expand",
    "workbench.iconTheme": "material-icon-theme",
    "gruvboxMaterial.darkContrast": "hard",
    "gruvboxMaterial.darkCursor": "green",
    "gruvboxMaterial.darkPalette": "mix",
    "gruvboxMaterial.darkSelection": "green",
    "gruvboxMaterial.darkWorkbench": "flat",
    "gruvboxMaterial.diagnosticTextBackgroundOpacity": "12.5%",
    "gruvboxMaterial.italicKeywords": true,
    "gruvboxMaterial.lightContrast": "soft",
    "gruvboxMaterial.lightCursor": "green",
    "gruvboxMaterial.lightPalette": "mix",
    "gruvboxMaterial.lightSelection": "green",
    "gruvboxMaterial.lightWorkbench": "flat",
    "editor.detectIndentation": false,
    "gitlens.defaultDateFormat": "Do, MMMM, YYYY; h:m",
    "gitlens.defaultDateShortFormat": "D, MM, YYYY; h:m",
    "gitlens.defaultTimeFormat": "h:m",
    "git.confirmSync": false,
    "git.autofetch": true,
    "vim.enableNeovim": true,
    "terminal.integrated.fontFamily": "BlexMono Nerd Font",
    "explorer.confirmDelete": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "material-icon-theme.hidesExplorerArrows": true,
    "material-icon-theme.showUpdateMessage": true,
    "material-icon-theme.activeIconPack": "react_redux",
    "explorer.decorations.badges": false,
    "projectManager.git.baseFolders": [
        "/home/axolotl/Projects"
    ],
    "editor.wrappingIndent": "indent",
    "editor.suggest.showWords": false,
    "editor.wordBasedSuggestions": false,
    "editor.smoothScrolling": true,
    "editor.cursorSmoothCaretAnimation": true,
    "files.autoSave": "afterDelay",
    "better-comments.highlightPlainText": true,
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "?",
            "color": "#3498DB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "note",
            "color": "#98C379",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        }
    ],
    "workbench.productIconTheme": "material-product-icons",
    "workbench.colorCustomizations": {},
    "workbench.preferredDarkColorTheme": "Material Theme Ocean",
    "workbench.preferredLightColorTheme": "Soft Material",
    "materialTheme.accent": "Indigo",
    "terminal.integrated.fontSize": 13,
    "editor.multiCursorModifier": "ctrlCmd",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "terminal.integrated.defaultProfile.linux": "zsh",
    "python.languageServer": "Pylance",
    "diffEditor.ignoreTrimWhitespace": false,
    "explorer.confirmDragAndDrop": false,
    "githubPullRequests.fileListLayout": "tree",
    "editor.scrollbar.horizontalScrollbarSize": 10,
    "editor.scrollbar.verticalScrollbarSize": 12,
    "editor.scrollbar.horizontal": "hidden",
    "translator.captureWord": false,
    "material-icon-theme.folders.color": "#ededed",
    "workbench.colorTheme": "Tokyo Night",
    "codesnap.showWindowControls": false,
    "codesnap.backgroundColor": "#ededed",
    "vim.leader": "<space>",




    // VIM
    "vim.visualModeKeyBindings": [
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
        {
            "before": ["<leader>", "y"],
            "commands": [
                "editor.action.clipboardCopyAction"
            ]
        },
    ],

    "vim.normalModeKeyBindings": [
        { 
                "before": ["/"],
                "commands": [
                    "actions.find"
                ]
        },
        {
            "before": ["<leader>", "p"],
            "commands": [
                "editor.action.clipboardPasteAction"
            ]
        },
        {
            "before": ["<leader>", "y"],
            "commands": [
                "editor.action.clipboardCopyAction"
            ]
        },
        {
            "before": ["u"],
            "commands": [
                "undo"
            ]
        },
        {
            "before": ["<C-r>"],
            "commands": [
                "redo"
            ]
        },
        {
            "before": ["<C-q>"],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        
    ],
    "projectManager.tags": [
        "Personal",
        "Work",
        "Courses"
    ],
    "awooga.enabled": {
        "rs": false
    },
    "codesnap.shutterAction": "copy",
    "rust-analyzer.cargo.allFeatures": true

}