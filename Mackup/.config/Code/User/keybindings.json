[
  {
    "key": "ctrl+shift+alt+i",
    "command": "editor.action.inspectTMScopes"
  },
  {
    "key": "ctrl+meta+f5",
    "command": "python.execInTerminal"
  },
  {
    "key": "ctrl+k shift+s",
    "command": "codesnap.start"
	},
	{
		"key": "ctrl+k shift+t",
		"command": "workbench.action.terminal.openNativeConsole",
		"when": "!terminalFocus"
	},
	{
		"key": "ctrl+shift+c",
		"command": "-workbench.action.terminal.openNativeConsole",
		"when": "!terminalFocus"
	}
]